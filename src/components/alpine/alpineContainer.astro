---
import alpineLogo from "../../assets/logos/alpine.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import Alpine from "./alpine.astro";

const { default: code } = await import("./alpine.astro?raw");
---

<FrameworkContainer framework="alpine" title="Alpine.js">
  <Alpine />
  <CodeBlock code={code} displayName="Alpine.js" lang="astro" logo={alpineLogo} />
</FrameworkContainer>
