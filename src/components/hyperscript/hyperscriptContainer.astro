---
import hyperscriptLogo from "../../assets/logos/hyperscript.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import Hyperscript from "./hyperscript.astro";

const { default: code } = await import("./hyperscript.astro?raw");
---

<FrameworkContainer 
  framework="hyperscript" 
  title="Hyperscript"
>
  <Hyperscript />
  <CodeBlock code={code} displayName="Hyperscript" lang="astro" logo={hyperscriptLogo} />
</FrameworkContainer> 