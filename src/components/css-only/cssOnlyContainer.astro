---
import cssLogo from "../../assets/logos/css.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import CssOnly from "./cssOnly.astro";

const code = await import("./cssOnly.astro?raw").then((m) => m.default);
---

<FrameworkContainer 
  framework="css-only" 
  title="CSS Only"
>
  <CssOnly />
  <CodeBlock code={code} displayName="CSS Only" lang="astro" logo={cssLogo} />
</FrameworkContainer> 