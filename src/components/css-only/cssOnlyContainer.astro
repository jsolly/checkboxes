---
import FrameworkContainer from "../FrameworkContainer.astro";
import CssOnly from "./cssOnly.astro";
import CodeBlock from "../CodeBlock.astro";
import cssLogo from "../../assets/logos/css.svg";

const code = await import('./cssOnly.astro?raw').then(m => m.default);
---

<FrameworkContainer title="CSS-only">
  <CssOnly />
  <CodeBlock 
    code={code} 
    displayName="CSS Only" 
    lang="html"
    logo={cssLogo}
  />
</FrameworkContainer> 