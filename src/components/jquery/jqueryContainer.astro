---
import jqueryLogo from "../../assets/logos/jquery.svg";
import CodeBlock from "../../components/shared/CodeBlock.astro";
import FrameworkContainer from "../../components/shared/FrameworkContainer.astro";
import PerformanceBadge from "../../components/shared/PerformanceBadge.astro";
import JQuery from "./jquery.astro";
import stats from "./stats.json";

const code = await import("./jquery.astro?raw").then((m) => m.default);
---
<script is:inline src="/src/scripts/jquery.min.js"></script>

<FrameworkContainer framework="jquery" title="jQuery">
  <PerformanceBadge 
    renderTime={stats.renderTime} 
    bundleSize={stats.bundleSize}
  />
  <JQuery />
  <CodeBlock 
    code={code} 
    displayName="jQuery" 
    lang="javascript" 
    logo={jqueryLogo}
  />
</FrameworkContainer> 