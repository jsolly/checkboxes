---
import FrameworkContainer from "../FrameworkContainer.astro";
import JQuery from "./jquery.astro";
import CodeBlock from "../CodeBlock.astro";
import jqueryLogo from "../../assets/logos/jquery.svg";

const code = await import('./jquery.astro?raw').then(m => m.default);
---

<FrameworkContainer title="jQuery">
  <JQuery />
  <CodeBlock 
    code={code} 
    displayName="jQuery" 
    lang="javascript" 
    logo={jqueryLogo}
  />
</FrameworkContainer> 