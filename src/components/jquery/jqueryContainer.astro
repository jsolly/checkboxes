---
import jqueryLogo from "../../assets/logos/jquery.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import JQuery from "./jquery.astro";

const { default: code } = await import("./jquery.astro?raw");
---

<FrameworkContainer 
  framework="jquery" 
  title="jQuery"
>
  <JQuery />
  <CodeBlock 
    code={code} 
    displayName="jQuery" 
    lang="javascript" 
    logo={jqueryLogo}
  />
</FrameworkContainer> 