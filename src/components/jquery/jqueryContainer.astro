---
import jqueryLogo from "../../assets/logos/jquery.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import JQuery from "./jquery.astro";

const code = await import("./jquery.astro?raw").then((m) => m.default);
---
<script is:inline src="/src/scripts/jquery.min.js"></script>

<FrameworkContainer 
  framework="jquery" 
  title="jQuery"
>
  <JQuery />
  <CodeBlock 
    code={code} 
    displayName="jQuery" 
    lang="javascript" 
    logo={jqueryLogo}
  />
</FrameworkContainer> 