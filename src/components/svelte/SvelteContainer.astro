---
import svelteLogo from "../../assets/logos/svelte.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import NestedCheckboxes from "./SvelteNestedCheckboxes.svelte";

const { default: code } = await import("./SvelteNestedCheckboxes.svelte?raw");
---

<FrameworkContainer 
  framework="svelte" 
  title="Svelte"
>
  <NestedCheckboxes client:only="svelte" />
  <CodeBlock 
    code={code} 
    displayName="Svelte" 
    lang="svelte" 
    logo={svelteLogo}
  />
</FrameworkContainer> 