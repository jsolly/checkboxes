---
import FrameworkContainer from "../FrameworkContainer.astro";
import NestedCheckboxes from "./NestedCheckboxes.svelte";
import CodeBlock from "../CodeBlock.astro";
import svelteLogo from "../../assets/logos/svelte.svg";

const code = await import('./NestedCheckboxes.svelte?raw').then(m => m.default);
---

<FrameworkContainer framework="svelte" title="Svelte">
  <NestedCheckboxes client:only="svelte" />
  <CodeBlock 
    code={code} 
    displayName="Svelte" 
    lang="typescript" 
    logo={svelteLogo}
  />
</FrameworkContainer> 