---
import FrameworkContainer from "../FrameworkContainer.astro";
import VanillaJS from "./vanilla.astro";
import CodeBlock from "../CodeBlock.astro";
import javascriptLogo from "../../assets/logos/javascript.svg";

const code = await import('./vanilla.astro?raw').then(m => m.default);
---

<FrameworkContainer title="Vanilla JavaScript">
  <VanillaJS />
  <CodeBlock 
    code={code} 
    displayName="Vanilla JS" 
    lang="javascript" 
    logo={javascriptLogo}
  />
</FrameworkContainer> 
