---
import vanillaLogo from "../../assets/logos/javascript.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import Vanilla from "./vanilla.astro";

const code = await import("./vanilla.astro?raw").then((m) => m.default);
---

<FrameworkContainer 
	framework="vanilla-js" 
	title="Vanilla JavaScript"
>
	<Vanilla />
	<CodeBlock code={code} displayName="Vanilla JS" lang="astro" logo={vanillaLogo} />
</FrameworkContainer> 
