---
import reactLogo from "../../assets/logos/react.svg";
import CodeBlock from "../CodeBlock.astro";
import FrameworkContainer from "../FrameworkContainer.astro";
import PerformanceBadge from "../PerformanceBadge.astro";
import NestedCheckboxes from "./ReactNestedCheckboxes";
import stats from "./stats.json";

const code = await import("./ReactNestedCheckboxes.jsx?raw").then(
	(m) => m.default,
);
---

<FrameworkContainer framework="react" title="React">
  <PerformanceBadge 
    renderTime={stats.renderTime} 
    bundleSize={stats.bundleSize}
  />
  <NestedCheckboxes client:only="react" />
  <CodeBlock 
    code={code} 
    displayName="React.js" 
    lang="jsx" 
    logo={reactLogo}
  />
</FrameworkContainer> 
