---
import reactLogo from "../../assets/logos/react.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import ReactNestedCheckboxes from "./ReactNestedCheckboxes";

const code = await import("./ReactNestedCheckboxes.jsx?raw").then(
	(m) => m.default,
);
---

<FrameworkContainer 
  framework="react" 
  title="React"
>
  <ReactNestedCheckboxes client:only="react" />
  <CodeBlock code={code} displayName="React" lang="jsx" logo={reactLogo} />
</FrameworkContainer> 
