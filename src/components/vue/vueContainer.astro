---
import vueLogo from "../../assets/logos/vue.svg";
import CodeBlock from "../shared/CodeBlock.astro";
import FrameworkContainer from "../shared/FrameworkContainer.astro";
import VueNestedCheckboxes from "./VueNestedCheckboxes.vue";

const { default: code } = await import("./VueNestedCheckboxes.vue?raw");
---

<FrameworkContainer 
  framework="vue" 
  title="Vue"
>
  <VueNestedCheckboxes client:only="vue" />
  <CodeBlock 
    code={code} 
    displayName="Vue" 
    lang="vue" 
    logo={vueLogo}
  />
</FrameworkContainer> 