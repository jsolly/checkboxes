---
import FrameworkContainer from "../FrameworkContainer.astro";
import NestedCheckboxes from "./NestedCheckboxes.vue";
import CodeBlock from "../CodeBlock.astro";
import vueLogo from "../../assets/logos/vue.svg";

const code = await import('./NestedCheckboxes.vue?raw').then(m => m.default);
---

<FrameworkContainer framework="vue" title="Vue.js">
  <NestedCheckboxes client:only="vue" />
  <CodeBlock 
    code={code} 
    displayName="Vue.js" 
    lang="vue" 
    logo={vueLogo}
  />
</FrameworkContainer> 